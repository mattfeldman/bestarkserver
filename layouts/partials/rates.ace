.ui.vertical.tertiary.stripe.segment#rates
  h1.ui.header.container
    i.tasks.icon
    .content Rates
      .sub.header Friendly PVE Rates
  .ui.container
    .ui.four.statistics
      {{ range .Site.Data.ark.data.rates }}
        .ui.statistic
          .value
            {{ .rate }}
          .label
            i.ui.icon class={{.icon}}
            {{ .label }}
      {{ end }}